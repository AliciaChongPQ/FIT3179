{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Incidence Rate For Cholera By State",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://raw.githubusercontent.com/AliciaChongPQ/FIT3179/refs/heads/main/Week%209%20Homework/js/my.json",
      "format": {"type": "geojson"}
    },
    "layer": [{
        "data": {
          "url": "my.json",
          "format": {
            "type": "json",
            "property": "features"
          }
        },
        "mark": {
          "type": "geoshape",
          "stroke": "#d3d9e1",  
          "strokeWidth": 0.3   
  
        },
        "encoding": {
            "color": {
      "field": "expenditure-to-income ratio",
      "type": "quantitative",
      "scale": {
            "domain": [0.2, 1.1], 
            "range": ["#fee0d2", "#de2d26"] 
          },
      "title": "Expenditure-To-Income Ratio"
    } ,
          "tooltip": [
            {"field": "properties.name", "type": "nominal", "title": "State"},
            {"field": "expenditure-to-income ratio", "type": "quantitative", "format": ".4f", "title": "Expenditure-to-Income Ratio"}
            
          ]
        },
        "transform": [
        {
          "lookup": "properties.feature",  
          "from": {
            "data": {
              "url": "cholera_by_state.csv"  
            },
            "key": "state",  
            "fields": ["expenditure-to-income ratio"]
          }
        }
      ]
      }]
  }