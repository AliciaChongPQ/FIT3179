{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Incidence Rate For Cholera By State",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://raw.githubusercontent.com/AliciaChongPQ/FIT3179/refs/heads/main/Week%209%20Homework/js/ne_1.json",
      "format": {"type": "topojson", "feature": "ne_1"}
    },
    "transform": [
      {
        "lookup": "properties.name",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/AliciaChongPQ/FIT3179/refs/heads/main/Week%209%20Homework/data/cholera_by_state.csv"
          },
          "key": "State",
          "fields": ["Year", "Incidence Rate Per 100 000 Population"]
        }
      }
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Incidence Rate Per 100 000 Population",
        "type": "quantitative",
        "scale": {"type": "linear"}
      },
      "tooltip": [
        {"field": "properties.name", "type": "nominal", "title": "State"},
        {"field": "Incidence Rate Per 100 000 Population", "type": "quantitative"}
      ]
    }
  }